{
    "name": "tongwoojun/flarum-ext-auth-iqiyi",
    "description": "Allow users to log in with iqiyi!",
    "authors": [
        {
            "name": "tongwoojun",
            "email": "tongwoojun@gmail.com",
            "homepage": "https://github.com/tongwoojun"
        }
    ],
    "keywords": [
        "flarum",
        "friendsofflarum",
        "oauth",
        "iqiyi"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": "^1.0.0",
        "fof/extend": "^1.0.0",
        "fof/components": "^1.0.0",
        "league/oauth2-client": "^2.0",
        "tongwoojun/oauth2-iqiyi": "1.0.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Tongwoojun\\OAuth\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "IQIYI OAuth",
            "category": "feature",
            "icon": {
                "name": "fas fa-sign-in-alt",
                "backgroundColor": "#e74c3c",
                "color": "#fff"
            }
        }
    },
    "repositories": {
        "packagist.org": false,
        "private-packagist": {
            "type": "composer",
            "url": "https://repo.packagist.com/tongwoojun/"
        }
    }
}
