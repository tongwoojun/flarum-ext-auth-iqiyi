{
    "name": "tongwoojun/flarum-ext-auth-iqiyi",
    "description": "Allow users to log in with GitHub, Twitter, Facebook, Google, and more!",
    "authors": [
        {
            "name": "tongwoojun",
            "email": "tongwoojun@gmail.com",
            "homepage": "https://github.com/tongwoojun"
        }
    ],
    "keywords": [
        "flarum",
        "friendsofflarum",
        "oauth",
        "iqiyi"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": "^1.0.0",
        "fof/extend": "^1.0.0",
        "fof/components": "^1.0.0",
        "tongwoojun/oauth2-iqiyi": "^1.8.2"
    },
    "replace": {
        "flarum/auth-facebook": "*",
        "flarum/auth-github": "*",
        "flarum/auth-twitter": "*",
        "fof/auth-discord": "*",
        "fof/auth-gitlab": "*",
        "luuhai48/oauth-google": "*",
        "luuhai48/oauth-linkedin": "*"
    },
    "autoload": {
        "psr-4": {
            "Tongwoojun\\OAuth\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "FoF OAuth",
            "category": "feature",
            "icon": {
                "name": "fas fa-sign-in-alt",
                "backgroundColor": "#e74c3c",
                "color": "#fff"
            }
        },
        "flagrow": {
            "discuss": "https://discuss.flarum.org/d/25182"
        }
    }
}
